* {
    font-size: 14px;
}

.QFrame {
    border: none;
}

#frame_border_left,
#frame_border_right,
#frame_border_top,
#frame_border_bottom,
#frame_border_topLeft,
#frame_border_topRight,
#frame_border_bottomLeft,
#frame_border_bottomRight,
#frame_titleBar,
#frame_central {
    background: #f0f0f0;;
}

#frame_border_left {
    border-left: 1px solid #000000;
}
#frame_border_right {
    border-right: 1px solid #000000;
}
#frame_border_top {
    border-top: 1px solid #000000;
}
#frame_border_bottom {
    border-bottom: 1px solid #000000;
}
#frame_border_topLeft {
    border-top: 1px solid #9c9c9c;
    border-left: 1px solid #9c9c9c;
}
#frame_border_topRight {
    border-top: 1px solid #9c9c9c;
    border-right: 1px solid #9c9c9c;
}
#frame_border_bottomLeft {
    border-bottom: 1px solid #9c9c9c;
    border-left: 1px solid #9c9c9c;
}
#frame_border_bottomRight {
    border-bottom: 1px solid #9c9c9c;
    border-right: 1px solid #9c9c9c;
}




QLineEdit {
    border: 1px solid #aaaaaa;
    padding: 4px;
    background: #ffffff;
}
QLineEdit::hover {
    border: 1px solid #a0a0a0;
}
QLineEdit::focus {
    border: 1px solid #a0a0a0;
}


QPushButton {
    background: #3385dd;
    border: 1px solid #e6e6e6;
    padding: 6 16 6 16px;
    color: #f7f7f7;
}
QPushButton::hover {
    background: #3385ff;
}
QPushButton::pressed {
    background: #4496ff;
}

QToolButton {
    border: none;
    padding: 2 6 2 6px;
    background: transparent;
}
QToolButton::hover {
    background: #CBDEFA;
}
QToolButton::pressed {

}

