}

QRadioBox {

}

QGroupBox {

}

QComboBox {
    border: 1px solid #aaaaaa;
    padding: 4px;
}
QComboBox::editable {
    background: #ffffff;
}
QComboBox::!editable {
    background: qlineargradient(
        spread: repeat,
        x1:0, y1:0,
        x2:2, y2:0,
        stop:0 #e3e3e3,
        stop:1 #d4d4d4
    );
}
QComboBox::drop-down {
    border: none;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    background: transparent;
    width: 32px;
    border-left: 1px solid #d3d3d3;
}
QComboBox::down-arrow {
    image: url(:/image/drop_down.png);
}
QComboBox::hover {
    border: 1px solid #a0a0a0;
}
QComboBox::focus {
    border: 1px solid #a0a0a0;
}
QComboBox::drop-down::hover {
    background: #e3e3e3;
}

QTabWidget {
    background: red;
}
.QWidget {
    background: #f7f7f7;
}

QAbstractSpinBox {
